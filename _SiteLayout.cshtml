<!DOCTYPE html>
<html>
    <head>
        <title>VNB - @Page.Title</title>

        <!-- Add JQuery Library -->
        <script src="Jquery/js/jquery-1.8.3.js" type="text/javascript"></script>
        <script src="Jquery/js/jquery-ui-1.9.2.custom.js" type="text/javascript"></script>    
          
        <!-- Add mousewheel plugin (this is optional) -->
	    <script type="text/javascript" src="Jquery/Plugins/fancybox/lib/jquery.mousewheel-3.0.6.pack.js"></script>

	    <!-- Add fancyBox main JS and CSS files -->
	    <script type="text/javascript" src="Jquery/Plugins/fancybox/source/jquery.fancybox.js?v=2.1.4"></script>
	    <link rel="stylesheet" type="text/css" href="Jquery/Plugins/fancybox/source/jquery.fancybox.css?v=2.1.4" media="screen" />

	    <!-- Add Button helper (this is optional) -->
	    <link rel="stylesheet" type="text/css" href="Jquery/Plugins/fancybox/source/helpers/jquery.fancybox-buttons.css?v=1.0.5" />
	    <script type="text/javascript" src="Jquery/Plugins/fancybox/source/helpers/jquery.fancybox-buttons.js?v=1.0.5"></script>

	    <!-- Add Thumbnail helper (this is optional) -->
	    <link rel="stylesheet" type="text/css" href="Jquery/Plugins/fancybox/source/helpers/jquery.fancybox-thumbs.css?v=1.0.7" />
	    <script type="text/javascript" src="Jquery/Plugins/fancybox/source/helpers/jquery.fancybox-thumbs.js?v=1.0.7"></script>

	    <!-- Add Media helper (this is optional) -->
	    <script type="text/javascript" src="Jquery/Plugins/fancybox/source/helpers/jquery.fancybox-media.js?v=1.0.5"></script>

        <!-- Add My Script and Style Files -->
        <script src="Scripts/Script.js" lang="javascript"></script>  
        <link href="@Href("~/Css/Style.css")" rel="stylesheet" media="screen">
    </head>
    <body>
        <div id="container">
            @RenderPage("~/Shared/Layout/_Header.cshtml")

            <div id="content">
                @RenderBody()
            </div>

            @RenderPage("~/Shared/Layout/_Footer.cshtml")
        </div>
    </body>
</html>
